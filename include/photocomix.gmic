      elif $16==7 reverse[1,0] fx_compose_value[-1,0] $17,0
      elif $16==8 fx_compose_grainmerge[-1,0] $17
   elif $16==8 fx_compose_freeze[-1,0] $17 fi
      if $18==1 fx_smooth_anisotropic 60,$19,$20,$21,1.1,0.8,30,2,0,1,1,0 fi
  done a c done done

