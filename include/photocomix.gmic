elif $8==2 reverse fx_compose_luminance $9,$7
elif $8==3 fx_compose_colordoping $9,$7
elif $8==4 fx_compose_comix_color $9,$7,$10
elif $8==5 fx_compose_graphicolor $9,0,$7
elif $8==6 fx_compose_graphixcolor $9,$7
