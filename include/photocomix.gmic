fx_colorstamp :

repeat $! l[$>] split_opacity l[0]

  +norm
  if $1 otsu. 256 else threshold. $2% fi
   n. 0,255
  apply_curve. 1,0,0,101,33,170,229,255,255
  if $3==1 quantize.. $4 to_rgb b.. xy,$5
   fx_compose_darken 1,0 fi
   done a c done done

