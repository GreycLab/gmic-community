fx_scaledown3 :
if $1==0" && "$4==0 b x,{w/$5} b y,{h/$6}
elif $1==0" && "$4==1 b x,{w/$5} b y,{w/$6}
elif $1==0" && "$4==2 b x,{h/$5},b y,{w/$6}
elif $1==1" && "$4==0 b x,$2 b y,$3
elif $1==1" && "$4==1 b x,$2 b y,$3
elif $1==1" && "$4==2 b x,$2 b y,$3
fi

if $4==0
r $5,$6,2
elif $4==1
rs $5
elif $4==2
rs ,$6,2
fi

if $7==0
fx_unsharp 0,1.25,30,$8,0,$9,{$9/3},1,0,$10
fi

