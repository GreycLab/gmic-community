#@gui Nonscape - Grass : pr_nongrass, pr_nongrass_preview(1)*
#@gui : Sparsity = ~int(3,1,100)
#@gui : Color = ~color(36,144,30)
#@gui : Color Variation = ~int(10,0,100)
#@gui : Y Start = float(50,5,150)
