 fi

 modeswitched=0
 if {*,SPACE}" || "{*1,SPACE}" || "{*2,SPACE}" && "$start mode+=1 mode={$mode%2} modeswitched=1 wait 100 fi

 if $modeswitched
  +store. temp_switch_tile
  $switchmode_tile
