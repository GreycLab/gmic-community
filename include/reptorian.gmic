if narg($22) normalize_1=$22         else normalize_1=$normalize_0 fi
if narg($23) modulo_method_2={$23%4} else modulo_method_2=$modulo_method_1 fi
if narg($24) modvalue_a_2={abs($24)} else modvalue_a_2=$modvalue_a_1 fi
