fx_frblur:
repeat $! l[$>] if $9 ac "fx_solidify_td $10,$11,0,0,$12",rgb fi split_opacity 
iw={w}
ih={h}
hypo={sqrt($iw*$iw+$ih*$ih)}
per={($4/4)/100*$hypo}
l[0] 
if {$1==1} rgb2ryb
elif {$1==2} rgb2cmyk
elif {$1==3} rgb2hsi
elif {$1==4} rgb2hsl
elif {$1==5} rgb2hsv
elif {$1==6} rgb2lab
elif {$1==7} rgb2lch
fi
rep_frblur $2,$3,$per,$5,{$6+1},$7,$8
if {$1==1} ryb2rgb
elif {$1==2} cmyk2rgb
elif {$1==3} hsi2rgb
elif {$1==4} hsl2rgb
elif {$1==5} hsv2rgb
elif {$1==6} lab2rgb
elif {$1==7} lch2rgb
fi
endl l[1] rep_frblur $2,$3,$per,$5,{$6+1},$7,0 endl a c endl done

