vibrato:
foreach {
 n 0,255

 f "
  q=($8/100)*w;
  l=($9/100)*h;
  X=(($1?w-x+q:x+q)/w-.5) * 2 * 1/$5 * 1/$7;
  Y=(($2?h-y+l:y+l)/h-.5) * 2 * 1/$6 * 1/$7;
  U=sqrt($3);
  V=sqrt($4);
  Z=((X-Y) * (X-U) * (X+U) * (Y-V) * (Y+V))+ $10;
  C=(Z+((1-i/255)/(($3>1?$3:1/$3)*($4>1?$4:1/$4)))*$14)*(Z+((1-i/255)/(($3>1?$3:1/$3)*($4>1?$4:1/$4)))*$14);
  F=abs(C)>1?C-int(C):C;
  A=$12?F:1-F;
  B=$11?(i*A):A*200;
  E=$13/100*B+(1-$13/100)*i;
 "

 n 0,255
 mul $15
 mod 256
}
