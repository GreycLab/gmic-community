elif $use_hex_mode
  $nw,$nh,$od,3,"begin_t(

   hex_a=["$hex_color_a"];
   hex_b=["$hex_color_b"];

   const minv=im#-1;
   const maxv=iM#-1;

   const hpi=pi/2;

   contrast(a)=(
    b=a*pi-hpi;
    .5*a^2+.5*(sin(b)+1)/2;
   );
  );
  v=i0#-1;
  shade=v>=0?v/maxv:contrast((1-(v/minv)^(1/2)))^2;
  col=v>=0?hex_a:hex_b;
  col*shade;
 "
fi
k.
s z r $ow,$oh,1,100%,6
um cs_out
