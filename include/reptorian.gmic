ts={size($channel_ord)}

m "axis_func: f \"begin(
  const ww=w-1;
  const hh=h-1;
  const cx=ww/2;
  const cy=hh/2;
  const ang=$""1;
  const cos_ang=cos(ang);
  const sin_ang=sin(ang);
  const axis_mode=$""2;
  const sublevel=$""3;
  const value_shift=$""5*($""6?1:-1);
  rot_x(a,b)=a*cos_ang-b*sin_ang;
  rot_y(a,b)=a*sin_ang+b*cos_ang;
  $""4?(
