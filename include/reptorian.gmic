   if $contain_alpha
    f[$>] c<s-1?i(#i,x%$tile_width,y%$tile_height,z,c):i/$8*i(#i0,x%$tile_width,y%$tile_height,z,s-1)
   else
    f[$>] i(#i,x%$tile_width,y%$tile_height,z,c)
