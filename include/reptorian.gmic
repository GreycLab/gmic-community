use_double_u=0
use_hex_mode=0

sub={abs($3)+1}

ow={w}
oh={h}
od={d}
nw={round(w*$sub)}
nh={round(h*$sub)}

if narg($9)||narg($10)

 if ${is_image_arg\ $9} mode+=1
  pass$9 0
 fi
 if ${is_image_arg\ $10} mode+=1
  pass$10 0
 fi

 if $mode==2

  if w#-2>1&&h#-2>1
   colormap.. $13
   if ($11%3)==1
    {w#-2},1,1,1,u(0,1)
    pixelsort... +,x,[-1]
    rm.
   elif ($11%3)==2
    mirror.. x
   fi
   find_color_a=I(#-1,lyapunov_surface/minv*v_length_b,0,0,1);
  else
   if w#-2>1
    if ($11%3)==1
     {w#-2},1,1,1,u(0,1)
