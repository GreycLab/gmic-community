 rep_tuppers_self_K $pre_negation_a

 name tuppers_self_image_base
 store _persistent

 if $allow_mode_switch
  k_a,k_b,k_value_from_images=${}
 else
  ('${}')
  replace_str ",","\n\n"
  k_value_from_images={t}
  rm
 fi

fi

rm $_persistent

if $mode_K_a
 remove_named tuppers_image_mode_0
 if !$allow_mode_switch
  append y
 fi
