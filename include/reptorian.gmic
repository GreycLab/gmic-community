ti={$!-1}
rep_cstdmap {$9?-1*$8*100:$8*100}%,${1-7}
+f. i?1 store. chan

if $10==0
 if $11 +eq. 0 negate. -.. 1 *.. . n.. 0,255 *. 255 a[-2,-1] c
 else n. 0,255
 fi
elif $10==1 -. 1 *. {359.999999/iM#-1} $chan . if $11 . *. 255 fi a[^0-$ti] c
 if $12==0 sh. 2 *. .5 rm. hcy2rgb.
 elif $12==1 hsi2rgb.
