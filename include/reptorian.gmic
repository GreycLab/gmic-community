	fi

	cmd_$0_calc_gray_image "
		begin(
			const red_weight_init=$1;
			const green_weight_init=$2;
			const blue_weight_init=$3;
			const total_weight=red_weight_init+green_weight_init+blue_weight_init;
			if($4,
