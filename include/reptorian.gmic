			):(
				pixel_detected(xp,yp)=
					det_px(i(#0,xp-1,yp-1,0,0,0,2))||
					det_px(i(#0,xp-1,yp+1,0,0,0,2))||
					det_px(i(#0,xp+1,yp-1,0,0,0,2))||
					det_px(i(#0,xp+1,yp+1,0,0,0,2));
			);

			"$altern_text_a"
		);

		temp_vec=I;

		xp=temp_vec[0];
		yp=temp_vec[1];

		repeat(limit_of_attempts,
			pv=v(0,8,1,0);

			xp+=new_pos_x[pv];
			yp+=new_pos_y[pv];

			if(pixel_detected(xp,yp),
