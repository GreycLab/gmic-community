	tcr,convert_back,contain_alpha=3,0,{s==2||s==4}

	if (s==3||s==4)&&$1
		convert_back=1
		if $not_cmyk
			$convert_colors_fwd
		else
			if s==3 tcr=4 rgb2cmyk
			elif s==4 tcr=4 s c,-3 rgb2cmyk.. a c
			fi
		fi
	fi

	if $contain_alpha&&!$2 shared 0,{s-2} fi

	rep_reverse_digits. ${3-4}
	if $5&&!$4 cut. 0,255 fi
	rm.

	if $convert_back
		if $not_cmyk
			$convert_colors_bwd
		else
			if s==4 cmyk2rgb
			elif s==5 s c,-4 cmyk2rgb.. a c
			fi
		fi
	fi
}
