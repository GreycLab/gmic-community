  $ref_rand_col_a
  $ref_rand_col_b
  $nw,$nh,$od,3,"begin_t(
    const minv=im#-3;
    const maxv=iM#-3;
    const ww_a=w#-2-1;
    const ww_b=w#-1-1;
   );
   lyapunov_surface=i0#-3;
   use_b=lyapunov_surface>=0;
   use_b?(I(#-1,lyapunov_surface/maxv*ww_b,0,0,1);)
        :(I(#-2,lyapunov_surface/minv*ww_a,0,0,1););
   "
   cs_out.
 else
  $ref_colors
  $nw,$nh,$od,3,"begin(
   const minv=im#-2;
   const maxv=iM#-2;
   const vs=w#-1-1;
   const diff=(maxv-minv)/vs;
  );
  lyapunov_surface=(i0#-2-minv)/diff;
