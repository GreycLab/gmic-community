#@cli : Return Roman Numeral as integer number.
rep_roman2int:
skip ${2=1}
struppercase $1

eval "
	started=char_changed=pos=old_pos=ans=num=0;
	old_relative_pos=-1;
	const _H=_'H';

	roman_chars='IVXLCDM';
	roman_num='"${}"';
	const chars_count=size(roman_chars);

	const is_strict=$2;
	num_vals=expr('(x&1?5:1)*10^(x>>1)',chars_count);
	char_freq_list=vector(#chars_count,0);
	ref_num_vals=[2,0,3,4,5,6,1];

	const roman_num_length=size(roman_num);
	const dec_roman_num_length=roman_num_length-1;

	repeat(roman_num_length,p,
		relative_pos=roman_num[dec_roman_num_length-p];

		if(changed_pos=(relative_pos!=old_relative_pos),
			if(started&&!char_changed,char_changed=changed_pos);
			sel=relative_pos&0b111;
			if(sel==4,sel>>=relative_pos>_H;);
			pos=ref_num_vals[sel];
			if(relative_pos!=roman_chars[pos],run('error inv_char_det'););
			if(is_strict,
				if(pos-old_pos<-2,
					run('error inv_char_gap');
				);
			);
			num=num_vals[pos];
		);

		if(pos<old_pos,last_relevant_pos=old_pos);
		if(!changed_pos?pos<last_relevant_pos,
			run('error excess_char');
		);

		encounters=++char_freq_list[pos];
		if(encounters>(3+char_changed),
			run('error excess_char');
		);

		if(num<<2<ans,ans-=num,ans+=num);
		old_relative_pos=relative_pos;
		old_pos=pos;
		started=1;
	);

	if(ans>3999,run('error exc_val'););

	if(is_strict,

		thousands=int(ans/1000);
		hundreds=int(ans%1000/100);
		tens=int(ans%100/10);
		ones=ans%10;
		place=0;

		test_vector=vector(#roman_num_length,0);

		process_section(value,low,mid,high,top)=if(value,
			value==9||value==4?(
				copy(test_vector[place],(value>5?top:mid),2);
				place+=2;
			):
			value>4?(
				num_of_chars=value-4;
				copy(test_vector[place],high,num_of_chars);
				place+=num_of_chars;
			):(
				num_of_chars=value;
				copy(test_vector[place],low,num_of_chars);
				place+=num_of_chars;
			);
		);

		if(thousands,
			copy(test_vector[0],'MMM',thousands);
			place+=thousands;
		);

		process_section(hundreds,'CCC','CD','DCCC','CM');
		process_section(tens,'XXX','XL','LXXX','XC');
		process_section(ones,'III','IV','VIII','IX');

		if(roman_num!=test_vector,run('error inv_char_set'););

	);

	ans;"
