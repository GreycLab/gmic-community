        out2display_alt. $canvas_set
        w[-1] {w#0},{h#0},0,$main_w_title rm.
       fi
       tempaltx=$altx tempalty=$alty
      else
       +out2display_alt[0] $canvas_set
       w[-1] {w#0},{h#0},0,$main_w_title rm.
