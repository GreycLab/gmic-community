       if $__altsymmetry_mode f. begin(ww=w-1;ww+=$__altsymmetry_posx;);x==(ww-$acsx)?0:i; fi
       __vmax_x={iM#-1}
       __pmin={find([crop(#-1)],1,0,1)}
       pmax={find([crop(#-1)],1,$iscl,-1)}
       __pdiff={$pmax-$__pmin+1}
       store. __isc
      else
