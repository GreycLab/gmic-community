     rep_trigmap_8i. {($channel_formula)[$>]+5*$>},{($multiplier)[$>]},{($valueshift)[$>]},{($midgray_expf)[$>]},{($lightness_merge_factor)[$>]},{($lightness_end_expf)[$>]},{($gray2col_end)[$>]}
     rm.
    done
   endl done
  elif '$1'=='8il'
   repeat $! l[$>]
    if s==3 to_gray to_rgb
    else to_graya to_rgba
    fi
    repeat 3
     sh. {($channel_order)[$>]}
     rep_trigmap_8i. {($channel_formula)[$>]+5*$>},{($multiplier)[$>]},{($valueshift)[$>]},{($midgray_expf)[$>]},{($lightness_merge_factor)[$>]},{($lightness_end_expf)[$>]},{($gray2col_end)[$>]}
     rm.
    done
   endl done
  else error ""(""$"1==8i")"||"(""$"1==8il")"==F"
  fi
 else
