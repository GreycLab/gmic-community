repeat $! l[$>]
 if h>w orientation=1
 else orientation=0
 fi
 if s==2||s>$tcr sh. {s-1} mina={im#-1} maxa={iM#-1} rm. fi
 rep_recpoltrans $point,-3
  if s==2||s>$tcr sh. {s-1} n. $mina,$maxa rm. fi
  rep_sptbwgp {$1*-1},1,$2,$3,$6
 rep_recpoltrans $point,2,$orientation
endl done
