 $ref_colors_a
 $ref_colors_b
 $nw,$nh,$od,{max(s#-2,s#-1)},"begin_t(
   const minv=im#-3;
   const maxv=iM#-3;
   const v_length_a=max(w#-2-1,h#-2-1);
   const v_length_b=max(w#-1-1,h#-2,-1);
